digraph G {
    subgraph axes {
    Axes->AxesPatch;
    AxesPatch[label="axesPatch\nRectangle"]

    Axes->XAxis;
    XAxis[label="xaxis\nXAxis"]

    Axes->YAxis;
    YAxis[label="yaxis\nYAxis"]    

    Axes->AxesTitle;
    AxesTitle[label="title\nText"]    

    Axes->AxesLines[label="0..n"];
    AxesLines[label="lines\n[Line2D]"]    

    Axes->AxesPatches[label="0..n"];
    AxesPatches[label="patches\n[Patch]"]    

    Axes->AxesTexts[label="0..n"];
    AxesTexts[label="texts\n[Text]"]    

    Axes->AxesTables[label="0..n"];
    AxesTables[label="tables\n[Table]"]    

    Axes->Legend;
    Legend[label="legend\nLegend"]    


    Axes->AxesImages[label="0..n"];
    AxesImages[label="images\n[AxesImage]"]    

    Axes->AxesCollections[label="0..n"];
    AxesCollections[label="collections\n[Collection]"]    


    label="Axes";
    }

    subgraph xaxis {
    XAxis->Xtick[label="0..n"];
    Xtick[label="majorTicks\n[Xtick]"];

    XAxis->MinorTicks[label="0..n"];
    MinorTicks[label="minorTicks\n[Xtick]"];

    XAxis->Xlabel;
    Xlabel[label="label\nText"];
    
    }


    subgraph xtick {

    Xtick->Xtick1line;
    Xtick1line[label="tick1line\nLine2D"];
    Xtick->Xtick2line;
    Xtick2line[label="tick2line\nLine2D"];

    Xtick->Xlabel1;
    Xlabel1[label="label1\nText"];
    Xtick->Xlabel2;
    Xlabel2[label="label2\nText"];

    Xtick->Xgridline;
    Xgridline[label="gridline\nLine2D"];

    
    

    }

    subgraph legend {
    Legend->LegendPatch;
    LegendPatch[label="legendPatch\nRectangle"];
    Legend->LegendTexts[label="0..n"];
    LegendTexts[label="texts\n[Text]"];

    Legend->LegendHandles[label="0..n"];
    LegendHandles[label="handles\n[Line2D or Patch]"];
    
    }


    Figure->FigureLines[label="0..n"];
    FigureLines[label="lines\n[Line2D]"];

    Figure->FigurePatches[label="0..n"];
    FigurePatches[label="patches\n[Patch]"];

    Figure->Axes[label="0..n"];
    Axes[label="axes\n[Axes]"];
    Figure->FigurePatch;
    FigurePatch[label="figurePatch\n[Rectangle]"]
 
    Figure->FigureTexts[label="0..n"];
    FigureTexts[label="texts\n[Text]"];

    Figure->FigureImages[label="0..n"];
    FigureImages[label="images\n[FigureImage]"];

    Figure->FigureLegend[label="0..n"];
    FigureLegend[label="legends\n[Legend]"];

    
}
